{
  "private": true,
  "name": "<%= config.name %>",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "wis dev",
    "build": "wis build"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/runtime": "7.16.7",
    "@babel/runtime-corejs3": "7.16.7",
    "@wisdesign/cli": "<%= packages['@wisdesign/cli'] %>",
    "@wisdesign/eslint-config": "<%= packages['@wisdesign/eslint-config'] %>",
    "@wisdesign/prettier-config": "<%= packages['@wisdesign/prettier-config'] %>",
    "@wisdesign/plugin-typescript": "<%= packages['@wisdesign/plugin-typescript'] %>",
    <% if (config.style === 'less') { %>
    "@wisdesign/plugin-less": "<%= packages['@wisdesign/plugin-less'] %>",
    <% } %>
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
}