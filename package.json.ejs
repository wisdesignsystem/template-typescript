{
  "private": true,
  "name": "<%= config.name %>",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "wis dev",
    "build": "wis build"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/runtime": "7.16.7",
    "@babel/runtime-corejs3": "7.16.7",
    "<%= packages['@wisdesign/cli'].name %>": "<%= packages['@wisdesign/cli'].version %>",
    "<%= packages['@wisdesign/eslint-config'].name %>": "<%= packages['@wisdesign/eslint-config'].version %>",
    "<%= packages['@wisdesign/prettier-config'].name %>": "<%= packages['@wisdesign/prettier-config'].version %>",
    "<%= packages['@wisdesign/plugin-typescript'].name %>": "<%= packages['@wisdesign/plugin-typescript'].version %>",
    <% if (config.style === 'less') { %>
    "<%= packages['@wisdesign/plugin-less'].name %>": "<%= packages['@wisdesign/plugin-less'].version %>",
    <% } %>
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.7"
  }
}